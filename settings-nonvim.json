{
  // ! VsCode Appearance
  "window.title": "NeoCode",
  "window.density.editorTabHeight": "compact",
  "window.restoreWindows": "none",
  "breadcrumbs.enabled": false,
  "editor.fontLigatures": false,
  "editor.minimap.enabled": false,
  "editor.fontFamily": "CaskaydiaMono Nerd Font Mono",
  "editor.lineNumbers": "relative",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.horizontal": "hidden",
  "editor.lineHeight": 25,
  "editor.fontSize": 16,
  "editor.lightbulb.enabled": "off",
  "editor.stickyScroll.enabled": false,
  "editor.cursorSurroundingLines": 10,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.fontSize": 16,
  "workbench.colorTheme": "Monokai Pro",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.editor.showTabs": "multiple",
  "workbench.sideBar.location": "right",
  "workbench.tips.enabled": false,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "blink",
  "editor.codeLens": false,
  "explorer.compactFolders": false,
  "workbench.activityBar.location": "hidden",
  "chat.commandCenter.enabled": false,
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "debug.showInlineBreakpointCandidates": false,
  "workbench.colorCustomizations": {
    "scrollbar.shadow": "#00000000",
    // ! Monokai Pro
    "[Monokai Pro]": {
      "titleBar.border": "#00000000",
      "sideBar.border": "#00000000",
      "statusBar.border": "#00000000",
      "editorGroup.border": "#00000000",
      "panel.background": "#2d2a2e",
      "terminal.background": "#2d2a2e",
    },
    // ! Tokyo Night
    "[Tokyo Night]": {
      "editorGroupHeader.border": "#16161e",
      "titleBar.border": "#16161e",
      "statusBar.border": "#16161e",
      "sideBar.border": "#16161e",
      "sideBarSectionHeader.border": "#16161e",
      "tab.border": "#16161e",
      "tab.activeBorder": "#1a1b26",
      "tab.unfocusedActiveBorder": "#1a1b26",
      "tab.activeBackground": "#1a1b26",
      "editorGroup.border": "#16161e",
      // "statusBar.background": "#565f89",
      // "statusBar.noFolderBackground": "#565f89",
      // "statusBar.debuggingBackground": "#565f89",
      // "statusBarItem.remoteBackground": "#565f89",
      // "statusBar.foreground": "#1a1b26",
      // "statusBar.noFolderForeground": "#1a1b26",
      // "statusBar.debuggingForeground": "#1a1b26",
      // "statusBarItem.remoteForeground": "#1a1b26",
    },
    // ! Rose Pine
    "[Ros√© Pine]": {
      "titleBar.inactiveBackground": "#191724",
      "terminal.background": "#191724",
      "panel.background": "#191724",
      "sideBar.border": "#26233a",
      "panel.border": "#26233a",
      "activityBar.border": "#26233a",
      "titleBar.border": "#26233a",
      "editorGroup.border": "#26233a",
      "statusBar.background": "#ebbcba",
      "statusBar.noFolderBackground": "#ebbcba",
      "statusBar.debuggingBackground": "#ebbcba",
      "statusBarItem.remoteBackground": "#ebbcba",
      "statusBar.foreground": "#191724",
      "statusBar.noFolderForeground": "#191724",
      "statusBar.debuggingForeground": "#191724",
      "statusBarItem.remoteForeground": "#191724"
    }
  },
  // ! VsCode Functionality
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.tabStopWidth": 2,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "liveServer.settings.donotShowInfoMsg": true,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editor.enablePreview": true,
  "editor.suggest.insertMode": "replace",
  "editor.tabSize": 2,
  "editor.linkedEditing": true,
  "editor.wordWrap": "off",
  "extensions.ignoreRecommendations": true,
  "git.openRepositoryInParentFolders": "never",
  "git.confirmSync": false,
  "git.autofetch": true,
  "workbench.settings.editor": "json",
  "security.allowedUNCHosts": ["wsl.localhost"],
  "workbench.panel.opensMaximized": "always",
  "workbench.startupEditor": "none",
  "workbench.editor.empty.hint": "hidden",
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "window.newWindowProfile": "Default",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.dialog.defaultPath": "C:\\Users\\ian\\Code\\",
  "files.simpleDialog.enable": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.showFoldingControls": "never",
  "editor.guides.indentation": false,
  "editor.renderLineHighlight": "none",
  "editor.overviewRulerBorder": false,
  "livePreview.openPreviewTarget": "External Browser",
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  // ! Prettier
  "prettier.printWidth": 180,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // ! Android Emulator
  "emulator.hasEditorTitleIcon": false,
  "emulator.emulatorPath": "C:\\Users\\ian\\AppData\\Local\\Android\\Sdk\\emulator",
  "emulator.emulatorPathWindows": "C:\\Users\\ian\\AppData\\Local\\Android\\Sdk\\emulator",
  "window.menuBarVisibility": "compact",
}
